{
  "name": "graphfinity",
  "version": "1.0.0",
  "description": "Graphfinity is the D3.js based reusable chart library. It has Cross-browser compatibility and each chart of this library is fully or configurable to perfectly match the look and feel of your website.",
  "main": "dist/graphfinity.module.js",
  "scripts": {
    "bootstrap": "npm install",
    "dev": "./node_modules/.bin/webpack-dev-server --config webpack.dev.config.js",
    "build": "npm run build:var && npm run build:module",
    "build:var": "./node_modules/.bin/webpack --config webpack.config.js",
    "build:module": "./node_modules/.bin/webpack --config webpack.module.config.js"
  },
  "keywords": [
    "Graphfinity"
  ],
  "author": "Rupinder Singh",
  "license": "MIT",
  "dependencies": {
    "d3": "^7.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@babel/preset-env": "^7.24.4",
    "babel-loader": "^9.1.3",
    "babel-plugin-module-resolver": "^5.0.2",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  }
}
